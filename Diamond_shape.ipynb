   "source": [
    "#Generating Diamond Shape With Python\n",
    "rows = 5\n",
    "for row in range(rows):\n",
    "    print(\" \"*(rows-row)+\" *\"*(row+1) )\n",
    "for row in range(rows-1):\n",
    "    print(\" \"*(row+2)+\" *\"*((rows-1)-row))"
   ]
